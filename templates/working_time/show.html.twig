{% extends 'base.html.twig' %}

{% block title %}WorkingTime{% endblock %}

{% block body %}
    <h1>WorkingTime</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ working_time.id }}</td>
            </tr>
            <tr>
                <th>Day_at</th>
                <td>{{ working_time.dayAt ? working_time.dayAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Hours</th>
                <td>{{ working_time.hours }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('working_time_index') }}">back to list</a>

    <a href="{{ path('working_time_edit', {'id': working_time.id}) }}">edit</a>

    {{ include('working_time/_delete_form.html.twig') }}
{% endblock %}
